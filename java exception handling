import java.io.*;
public class demo {
    public static void main(String[] args)
    {
        int x = 10;
        int y = 0;
        try
        {
            System.out.println(x / y);
        }
        catch(ArithmeticException e)
        {
            System.out.println(e.toString());
        }
    }
}

import java.io.*;
public class demo {
    public static void main(String[] args)
    {
        
        int x = 18;
        int y = 5;
        try
        {
            System.out.println(x / y);
        }
        catch(ArithmeticException e)
        {
            System.out.println(e.toString());
        }
    }
}
