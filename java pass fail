import java.util.Scanner;
public class Exp71{
    public static void main(String[] args)
    {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter score of the student: ");
        int num = sc.nextInt();
        if (num >= 40)
        {
            System.out.println("You are Pass!");
        }
        else
            System.out.println("You are Fail!");
    }
}
